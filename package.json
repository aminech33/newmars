{
  "name": "newmars",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently \"npm run backend\" \"npm run dev\"",
    "backend": "cd backend && python main.py",
    "build": "tsc -b && vite build",
    "build:nocheck": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:fast": "tauri build --no-bundle",
    "validate": "node scripts/validate-files.js",
    "backup": "node scripts/backup.js",
    "check-workspace": "node scripts/check-workspace.js",
    "pre-commit": "npm run validate",
    "safe-push": "npm run validate && git push",
    "security-check": "npm run check-workspace && npm run validate"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@monaco-editor/react": "^4.7.0",
    "@sentry/react": "^10.32.1",
    "@tauri-apps/api": "^2.9.1",
    "@tremor/react": "^3.18.7",
    "@types/crypto-js": "^4.2.2",
    "@types/react-grid-layout": "^1.3.6",
    "crypto-js": "^4.2.0",
    "elkjs": "^0.11.0",
    "framer-motion": "^12.23.25",
    "lucide-react": "^0.460.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-grid-layout": "^1.5.2",
    "react-resizable-panels": "^4.0.7",
    "reactflow": "^11.11.4",
    "recharts": "^3.5.1",
    "web-vitals": "^5.1.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@playwright/test": "^1.57.0",
    "@tauri-apps/cli": "^2.9.5",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "jsdom": "^27.4.0",
    "png-to-ico": "^3.0.1",
    "postcss": "^8.4.47",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.14",
    "terser": "^5.44.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vitest": "^4.0.16"
  }
